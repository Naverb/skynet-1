{
  "manifest_version": 2
, "version": "0.1"
, "name": "Skynet 0.1 Alpha"
, "app": {
    "background": {
      "scripts": ["skynet.js"]
    }
  }
, "permissions": [{
    "socket": [
      "udp-bind::5554"
    , "udp-send-to::5554"

    , "tcp-connect:*:5555"
    , "tcp-listen:*:5555"

    , "udp-bind::5556"
    , "udp-send-to::5556"

    , "udp-bind::5559"
    , "udp-send-to::5559"
    ]
  }]
}
